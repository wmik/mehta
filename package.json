    "@badrap/bar-of-progress": "^0.3.0",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@sentry/nextjs": "^10.39.0",
    "@trigger.dev/react-hooks": "^4.3.3",
    "@trigger.dev/sdk": "^4.3.3",
    "@types/bcryptjs": "^2.4.6",
    "ai": "^6.0.83",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.4",
    "lucide-react": "^0.563.0",
    "mixpanel": "^0.20.0",
    "mixpanel-browser": "^2.74.0",
    "next": "16.1.6",
    "next-auth": "^4.24.13",
    "next-themes": "^0.4.6",
    "ollama-ai-provider-v2": "^3.3.0",
    "pg": "^8.18.0",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
