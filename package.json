{
  "name": "mehta",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "description": "Mental Health Transcript Analysis",
  "scripts": {
    "trigger:dev": "npx trigger.dev@latest dev",
    "dev": "npx concurrently --kill-others --names \"next,trigger\" --prefix-colors \"yellow,blue\" \"next dev\" \"npm run trigger:dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "format": "prettier --write {src,__tests__}/**/*.{js,ts,tsx,json,css,md}",
    "format:check": "prettier --check",
    "db:seed": "tsx prisma/seed.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
